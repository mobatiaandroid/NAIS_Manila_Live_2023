<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <RelativeLayout
        android:id="@+id/relSub"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp" >
        <LinearLayout
            android:id="@+id/iconImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:orientation="horizontal"
            android:paddingBottom="5dp"
            android:weightSum="100"
            android:background="@color/split_bg">
            <LinearLayout
                android:layout_width="80dp"
                android:layout_height="60dp"
                android:orientation="horizontal"
                android:gravity="center"
                >
                <LinearLayout
                    android:id="@+id/startLinear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone"
                    >
                    <com.mobatia.nasmanila.manager.CustomFontSansProTextWhite
                        android:id="@+id/listDay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textSize="16dp"
                        android:text="10"
                        android:textColor="@color/white"
                        android:singleLine="true" />
                    <com.mobatia.nasmanila.manager.CustomFontSansProTextWhite
                        android:id="@+id/listMonth"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"

                        android:textSize="16dp"
                        android:text="May"
                        android:textColor="@color/white"
                        android:singleLine="true" />
                    <com.mobatia.nasmanila.manager.CustomFontSansProTextWhite
                        android:id="@+id/listYear"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"

                        android:textSize="16dp"
                        android:text="2017"
                        android:textColor="@color/white"
                        android:singleLine="true" />

                </LinearLayout>
                <com.mobatia.nasmanila.manager.CustomFontSansProTextWhiteBold
                    android:id="@+id/arrowMark"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:textSize="18dp"
                    android:text="-"
                    android:visibility="gone"

                    android:layout_gravity="center_vertical"
                    android:textColor="@color/white"
                    android:singleLine="true" />

                <LinearLayout
                    android:id="@+id/endLinear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone"
                    >
                    <com.mobatia.nasmanila.manager.CustomFontSansProTextWhite
                        android:id="@+id/listDayEnd"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"

                        android:textSize="16dp"
                        android:text="10"
                        android:textColor="@color/white"
                        android:singleLine="true" />
                    <com.mobatia.nasmanila.manager.CustomFontSansProTextWhite
                        android:id="@+id/listMonthEnd"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"

                        android:textSize="16dp"
                        android:text="May"
                        android:textColor="@color/white"
                        android:singleLine="true" />
                    <com.mobatia.nasmanila.manager.CustomFontSansProTextWhite
                        android:id="@+id/listYearEnd"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"

                        android:textSize="16dp"
                        android:text="2017"
                        android:textColor="@color/white"
                        android:singleLine="true" />

                </LinearLayout>

            </LinearLayout>
